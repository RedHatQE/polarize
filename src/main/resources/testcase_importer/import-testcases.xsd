          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="component"/>
      <xs:attribute name="subcomponent"/>
      <xs:attribute name="upstream">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="yes"/>
            <xs:enumeration value="no"/>
          </xs:restriction>
